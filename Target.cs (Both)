using UnrealBuildTool;
using System.Collections.Generic;

public class GameJamWhortlyTarget : TargetRules
{
	public GameJamWhortlyTarget( TargetInfo Target) : base(Target)
	{
		Type = TargetType.Game;
		DefaultBuildSettings = BuildSettingsVersion.V2;
		ExtraModuleNames.AddRange( new string[] { "GameJamWhortly" } );
	}
}

// Copyright Epic Games, Inc. All Rights Reserved.

using UnrealBuildTool;
using System.Collections.Generic;

public class GameJamWhortlyEditorTarget : TargetRules
{
	public GameJamWhortlyEditorTarget( TargetInfo Target) : base(Target)
	{
		Type = TargetType.Editor;
		DefaultBuildSettings = BuildSettingsVersion.V2;
		ExtraModuleNames.AddRange( new string[] { "GameJamWhortly" } );
	}
}


